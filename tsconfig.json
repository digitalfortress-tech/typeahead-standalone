{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist/",
    "module": "es2020",
    "target": "es2019",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "lib": [
      "es2020",
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [
      "cypress",
      "cypress-axe",
      "node",
      "vitest/globals"
    ]
  }
}